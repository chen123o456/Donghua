<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.example.meiketang.MainActivity">


    <ImageView
        android:id="@+id/aihaozhe"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:src="@mipmap/aihaozhe"
        android:onClick="aihaozhe"
        android:layout_centerInParent="true" />
    <ImageView
        android:id="@+id/meiyiren"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:onClick="meiyiren"
        android:src="@mipmap/meiyeren"
        android:layout_centerInParent="true" />
</RelativeLayout>
